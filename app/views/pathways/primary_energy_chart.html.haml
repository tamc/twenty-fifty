%script{ :type => 'text/javascript' }
  $(document).ready(function() {
  = scroll_to_last_position
  = redirect_to_disclaimer
  = draw_demand_chart
  = draw_primary_energy_chart
  = draw_emissions_chart
  })
  
#demand
  .chart#demand_chart 

  = render "demand_choices"

#supply
  .chart#supply_chart  
  .switch= link_to "Switch to chart showing just electricity", pathway_url(:id => @code)

  = render "supply_choices"
  
#ghg
  
  .chart#emissions_chart

  = render "ghg_choices"
  
  #keydata= render 'keydata'
      
.clear